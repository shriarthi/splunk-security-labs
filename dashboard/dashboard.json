  "dataSources": {
    "ds_t6pINfXa": {
      "type": "ds.search",
      "options": {
        "query": "index=web_logs sourcetype=custom_access_combined host=\"server1\" source=\"sample_weblogs.log\"\n| rex field=_raw \"^(?<clientip>\\S+)\"\n| top limit=10 clientip\n"
      },
      "name": "Search_1"
    },
    "ds_UBVpAKpd": {
      "type": "ds.search",
      "options": {
        "query": "index=web_logs sourcetype=custom_access_combined host=\"server1\" source=\"sample_weblogs.log\"\n| rex field=_raw \"\\\"\\w+\\s+(?<uri_path>[^\\s]+)\"\n| top limit=10 uri_path\n"
      },
      "name": "Search_2"
    },
    "ds_Fd2L6HBZ": {
      "type": "ds.search",
      "options": {
        "query": "index=web_logs sourcetype=custom_access_combined host=\"server1\" source=\"sample_weblogs.log\"\n| rex field=_raw \"\\\"\\w+\\s+[^\\s]+\\s+HTTP/[^\\\"]+\\\"\\s+(?<status>\\d+)\"\n| stats count by status\n| sort - count\n"
      },
      "name": "Search_3"
    },
    "ds_kuVbjRRU": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=custom_linux_secure host=\"server1\" source=\"sample_security.log\" \"Failed password\"\n| rex field=_raw \"from\\s+(?<src_ip>\\d+\\.\\d+\\.\\d+\\.\\d+)\"\n| stats count by src_ip\n| sort - count\n"
      },
      "name": "Search_4"
    },
    "ds_xcw4rou1": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=custom_linux_secure host=\"server1\" source=\"sample_security.log\" \"sudo:\"\n| rex field=_raw \"COMMAND=(?<command>.*)\"\n| stats count by command\n| sort - count\n"
      },
      "name": "Search_5"
    },
    "ds_MgmeZl1a": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=custom_linux_secure host=\"server1\" source=\"sample_security.log\" (\"Failed password\" OR \"Accepted\")\n| eval type=if(like(_raw, \"%Failed%\"), \"Failed\", \"Success\")\n| stats count by type\n"
      },
      "name": "Search_6"
    }
  },
  "defaults": {
    "dataSources": {
      "ds.search": {
        "options": {
          "queryParameters": {
            "latest": "$global_time.latest$",
            "earliest": "$global_time.earliest$"
          }
        }
      }
